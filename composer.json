{
    "name": "jacopo/library",
    "description": "General laravel library package",
    "license" : "MIT",
    "authors": [
        {
            "name": "jacopo",
            "email": "jacopo@jacopobeschi.com",
            "homepage": "http://www.jacopobeschi.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.0",
        "orchestra/testbench": "2.1.*",
        "way/laravel-test-helpers": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Jacopo\\Library\\": "src/"
        },
        "psr-4": {
            "Jacopo\\Library\\Tests\\" : "tests/"
        }
    },
    "minimum-stability": "stable"
}
